create table if not exists epiquery.test_table(
  id int,
  name varchar(25)
);
delete from epiquery.test_table;

insert into epiquery.test_table (id, name) values (1, "pants");
insert into epiquery.test_table (id, name) values (2, "slacks");

select id, name from epiquery.test_table;
