declare @test_table table
(
  id int,
  name varchar(25)
)

insert into @test_table (id, name) values(1, 'pants')
insert into @test_table (id, name) values(2, 'pants')


declare @test_table2 table
(
  id int,
  name varchar(25)
)

insert into @test_table2 (id, name) values(1, 'pants2')
insert into @test_table2 (id, name) values(2, 'pants2')

select * from @test_table
select * from @test_table2
